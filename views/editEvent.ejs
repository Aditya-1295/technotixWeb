<%- include('partials/header'); -%>





<div class="container-fluid mt-4">
    <div class="row justify-content-center">



        <% events.forEach(function(item){ %>

          <div class="col-lg-4 col-md-6 col-sm-12">
              <div class="card boder-dark" style="width: 18rem;">
                <img class="card-img-top" src="/image/<%= item.image %>" alt="Card image cap">
                  <div class="card-body">
                      <h5 class="card-title"> <%= item.eventName %> </h5>

                      <p class="card-text"><%= item.date %> </p>
                  </div>
                  <div class="card-footer">
                    <form action="/editEvent" method="post">
                      <small class="text-muted"><button type="submit" name="delete" value="<%= item._id %>">Delete</button></small>
                    </form>
                  </div>
              </div>
          </div>

        <% }) %>





    </div>
</div>



<script src="https://kit.fontawesome.com/14c951c111.js" crossorigin="anonymous"></script>
